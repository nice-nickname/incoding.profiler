{
  "manifest_version": 3,
  "name": "incoding.profiler",
  "description": "Devtools extension for profiling incoding.framework.js",

  "version": "1.0",
  "devtools_page": "index.html",

  "action": {
    "default_popup": "popup.html"
  },

  "background": {
    "service_worker": "background.js"
  },

  "permissions": [
    "activeTab", "scripting", "storage"
  ],

  "host_permissions": [
    "<all_urls>"
  ]
}
